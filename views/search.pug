extends layouts/main
include mixins/videoBlock

block content
    .search__header
        // get variables sent by form
        h3 Searching For #{searchingBy}
    .search_videos
        if videos.length === 0 
            h5 No Videos Found
        each item in videos
           +videoBlock({
               title:item.title,
               views:item.views,
               videoFile:item.fileUrl,
               id:item.id
           })